<!-- job listings index, show all the listing for agent side-->

<h4 style="padding: 25px; text-align: justify;"><%= @job_listings.count %> Motorbikes</h4>
<section class="listings">
  <div class="container-fluid">
    <div class="Cards"> <!--does not have css for the class yet-->
        <div class="col-md-8">
          <div class="cards">
            <% @job_listings.each do |job_listing|  %>
             <div class="grid-item">
                 <%= render 'shared/card_agent_Job_listing', job_listing: job_listing %>
             </div>
            <% end %>
          </div>
        </div>
        <div class="col-md-4">
          <div  id="map" style="width: 100%; height: 100vh; position: sticky; top: 0 " data-markers="<%= @markers.to_json %>" data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>
        </div>
    </div>
  </div>
</section>



<h6>No more listing!</h6>
<p>Try changing your filters</p>
